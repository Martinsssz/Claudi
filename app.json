{
  "expo": {
    "name": "Claudi",
    "slug": "claudi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoDarkTeste.png",
    "userInterfaceStyle": "automatic",
    "scheme": "claudi",
    "splash": {
      "image": "./assets/images/SplashScreen.png",
      "resizeMode": "stretch",
      "backgroundColor": "#D7E6F4"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/SplashScreen.png",
        "backgroundColor": "#D7E6F4"
      },
      "package": "com.pietrombatistela.claudi"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "8d9308b8-612d-4684-96e4-f01ca932ec59"
      }
    },
    "owner": "pietrombatistela"
  }
}
